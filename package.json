{
  "name": "com.powerdesign.whois",
  "displayName": "WHOIS Seeker",
  "version": "1.0.0",
  "description": "A simple app to perform WHOIS lookups on domain names.",
  "main": "main.js",
  "scripts": {
    "build:css": "tailwind build -i src/style.css -o www/tailwind.css"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Peter Suberu",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-android-support-gradle-release": "^3.0.1",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-dialogs": {},
      "cordova-android-support-gradle-release": {
        "ANDROID_SUPPORT_VERSION": "27.+"
      },
      "cordova-plugin-android-window-background": {
        "WINDOW_BACKGROUND_COLOR": "#d1fae5"
      }
    },
    "platforms": [
      "android"
    ]
  },
  "dependencies": {
    "cordova-plugin-android-window-background": "1.0.2",
    "tailwindcss": "^2.2.7"
  }
}
